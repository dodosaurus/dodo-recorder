{
  "name": "dodo-recorder",
  "version": "0.1.0",
  "description": "Record browser interactions and voice commentary for AI-assisted automated test generation",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "./build/generate-build-info.sh . && vite build && electron-builder --config electron-builder.test.json --mac --publish never -c.mac.identity=null",
    "build:prod": "dotenv -- ./build/generate-build-info.sh . && dotenv -- vite build && dotenv -- electron-builder --mac",
    "postinstall": "./build/install-playwright-browsers.sh",
    "install:browsers": "./build/install-playwright-browsers.sh",
    "install:browsers:all": "./build/install-playwright-browsers-all-platforms.sh",
    "generate-icons": "./build/generate-icons.sh"
  },
  "keywords": [
    "playwright",
    "testing",
    "recorder",
    "electron"
  ],
  "author": "Jozef Kovac <kovac.jozef25@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dodosaurus/dodo-recorder.git"
  },
  "bugs": {
    "url": "https://github.com/dodosaurus/dodo-recorder/issues"
  },
  "homepage": "https://github.com/dodosaurus/dodo-recorder#readme",
  "dependencies": {
    "electron-log": "^5.4.3",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "playwright": "^1.40.0",
    "uuid": "^9.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.4",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.2",
    "vite": "^5.0.7",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  }
}
